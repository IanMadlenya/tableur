<table>
    <thead>
        <tr>
            <th></th>
            <% for i in [65...91]: %>
                <th><%= String.fromCharCode i %></th>
            <% end %>
        </tr>
    </thead>
    <tbody>
        <% for i in [0...50]: %>
            <tr>
                <th><%= i %></th>
                <% for j in [65...91]: %>
                    <td data-cell="<%= String.fromCharCode(j) + i %>"></td>
                <% end %>
            </tr>
        <% end %>
    </tbody>
</table>